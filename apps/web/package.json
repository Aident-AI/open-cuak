{
  "name": "@aident/web",
  "version": "0.1.0",
  "private": true,
  "exports": {
    "./*": "./src/export-map.generated.ts"
  },
  "scripts": {
    "build": "next build",
    "db:fetch": "npx supabase db push --dry-run",
    "db:pull": "npx supabase db pull; npm run gen-types",
    "db:reset": "npx supabase db reset && npm run db:pull",
    "db:revert": "npx supabase migration repair --status reverted",
    "db:up": "npx supabase migration up && npm run gen-types",
    "dev": "npm run db:up && next dev",
    "docker:build": "docker build -t aident-ai/open-cuak-web -f Dockerfile ../../",
    "docker:run": "npm run docker:stop && docker run -p 3000:3000 --name open-cuak-web -v ./.env.production:/app/apps/web/.env.production aident-ai/open-cuak-web",
    "docker:start": "npm run build && npm run docker:build && npm run docker:run",
    "docker:stop": "docker rm -f open-cuak-web",
    "exec": "TS_NODE_PROJECT=./tsconfig.json ts-node -r tsconfig-paths/register",
    "format": "npx prettier . --write",
    "format-check": "npx prettier . --check",
    "function-serve": "npx supabase functions serve --env-file ./supabase/functions/.env.functions.local",
    "function-test": "deno run --allow-all",
    "gen-types": "npx supabase gen types typescript --local > ../../packages/shared/src/schema.gen.ts",
    "lint": "next lint",
    "repl": "npx ts-node --esm --interactive --compilerOptions '{\"target\": \"esnext\"}' --pretty",
    "start": "next start",
    "start-prod": "npx env-cmd -f .env.production next dev",
    "supabase": "npx supabase",
    "test-deno": "deno test --allow-all"
  },
  "dependencies": {
    "@aident/eslint-config": "workspace:*",
    "@aident/prettier-config": "workspace:*",
    "@aident/shared": "workspace:*",
    "@aws-sdk/client-cloudfront": "^3.709.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^2.1.2",
    "@microlink/react-json-view": "^1.23.0",
    "@mui/material": "^5.14.7",
    "@opentelemetry/api-logs": "^0.57.0",
    "@opentelemetry/instrumentation": "^0.57.0",
    "@opentelemetry/sdk-logs": "^0.57.0",
    "@vercel/analytics": "^1.1.1",
    "@vercel/functions": "^1.0.2",
    "@vercel/otel": "^1.10.0",
    "@vercel/speed-insights": "^1.0.2",
    "autoprefixer": "^10.4.14",
    "common-tags": "^1.8.2",
    "cookies-next": "^4.0.0",
    "cytoscape": "^3.30.2",
    "encoding": "^0.1.13",
    "flowbite": "^1.8.1",
    "google-auth-library": "^9.4.2",
    "googleapis": "^126.0.1",
    "html-to-text": "^9.0.5",
    "jsonlint-mod": "^1.7.6",
    "langsmith": "^0.2.14",
    "neo4j-driver": "^5.23.0",
    "next-client-cookies": "^1.1.0",
    "next-swagger-doc": "^0.4.0",
    "openai": "^4.3.0",
    "openapi3-ts": "^4.1.2",
    "puppeteer-core": "npm:rebrowser-puppeteer-core@^23.10.3",
    "react-audio-visualize": "^1.2.0",
    "react-cytoscapejs": "^2.0.0",
    "react-markdown": "^9.0.1",
    "react-scroll": "^1.8.9",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.5",
    "sse.js": "^0.6.1",
    "swagger-ui-react": "^5.10.3",
    "yauzl": "^3.1.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.14",
    "@types/common-tags": "^1.8.2",
    "@types/cytoscape": "^3.21.5",
    "@types/html-to-text": "^9.0.1",
    "@types/jsdom": "^21.1.6",
    "@types/react-cytoscapejs": "^1.2.5",
    "@types/react-scroll": "^1.8.7",
    "@types/swagger-ui-react": "^4.18.3",
    "@vercel/node": "^3.0.26",
    "tailwindcss-animated": "^1.0.1",
    "tsconfig-paths": "^4.2.0"
  }
}
